                    </td>
                     <td>
                       @reservation.ExemptionDate.ToString("dd-MM-yyyy")
                    </td>
                     <td>
                       @reservation.IsBreakfastIncluded.ToUpper()
                    </td>
                     <td>
                       @reservation.IsAllInclusive.ToUpper()
                    </td>
                     <td>
                       <ol>
                            @foreach (var client in reservation.Clients)
                            {
                               <li>@(client.FirstName + ' ' + client.LastName)</li>
                            }
