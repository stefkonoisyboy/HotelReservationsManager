<h1 class="text-center">
    ALL ROOMS
</h1>

<form asp-controller="Rooms" asp-action="AllFiltered">
    <input class="form-control" text="number" asp-for="@this.Model.FilterInputModel.Capacity" />
    <input class="form-control" asp-for="@this.Model.FilterInputModel.RoomType"/>
    <select asp-for="@this.Model.FilterInputModel.RoomType">
        @foreach(var roomType in this.Model.FilterInputModel.RoomItems)
        {
            <option value="@roomType">@roomType</option>
        }
    </select>
    
</form>

@{
    int number = 1;
}
<table class="table table-striped custom-table">
          <thead>
            <tr>
              <th scope="col">Room #</th>
              <th scope="col">Name</th>
              <th scope="col">Hotel Name</th>
              <th scope="col">RoomType</th>
              <th scope="col">IsFree</th>
              <th scope="col">Capacity</th>
              <th scope="col">Adult Price</th>
              <th scope="col">Child price</th>
              <th scope="col">Actions</th>

            </tr>
          </thead>
          <tbody>
              @foreach(var room in this.Model.Rooms)
              {
                  <tr scope="row" class="active">
                    <td>
                        @(number++)
                    </td>
                    <td>
                        @room.Name  
                    </td>
                    <td>
                        @room.HotelName  
                    </td>
                    <td>
                        @room.RoomType  
                    </td>
                    <td>
                        @room.IsFree  
                    </td>
                    <td>
                        @room.Capacity  
                    </td>
                    <td>
                        @room.AdultPrice  
                    </td>
                    <td>
                        @room.ChildPrice  
                    </td>
                    <td>
                        <a asp-action="ById" asp-controller="Rooms" asp-route-id="@room.Id">
                            <h6>
                               Details
                            </h6>    
                        </a>
                        <a class="btn btn-primary" asp-action="Delete" asp-controller="Rooms" asp-route-id="@room.Id">
                            <svg height="16" width="16" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 7l-.867 12.142A2 2 0 0116.138 21H7.862a2 2 0 01-1.995-1.858L5 7m5 4v6m4-6v6m1-10V4a1 1 0 00-1-1h-4a1 1 0 00-1 1v3M4 7h16"></path></svg>
                        </a>
                        <a class="btn btn-success" asp-action="UpdateRoom" asp-controller="Rooms" asp-route-id="@room.Id">
                            <svg height="16" width="16" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15.232 5.232l3.536 3.536m-2.036-5.036a2.5 2.5 0 113.536 3.536L6.5 21.036H3v-3.572L16.732 3.732z"></path></svg>
                        </a> 
                    </td>
                  </tr>

              }
          </tbody>
</table>
<div class="text-center">
<nav aria-label="...">
    <ul class="pagination justify-content-center">
        <li class="page-item @if (!Model.HasPreviousPage) { <text>disabled</text> }">
            <a class="page-link" asp-controller="Rooms" asp-action="AllFiltered" asp-route-id="@Model.PreviousPageNumber">Previous</a>
        </li>
        @for (int i = Model.PageNumber - 4; i < Model.PageNumber; i++)
        {
            if (i > 0)
            {
                <li class="page-item"><a class="page-link" asp-controller="Rooms" asp-action="AllFiltered" asp-route-page="@i">@i</a></li>
            }
        }
        <li class="page-item active" aria-current="page">
            <span class="page-link">
                @Model.PageNumber
                <span class="sr-only">(current)</span>
            </span>
        </li>
        @for (int i = Model.PageNumber + 1; i <= Model.PageNumber + 4; i++)
        {
            if (i <= Model.PagesCount)
            {
                <li class="page-item"><a class="page-link" asp-controller="Rooms" asp-action="AllFiltered" asp-route-page="@i">@i</a></li>
            }
        }
        <li class="page-item @if (!Model.HasNextPage) { <text>disabled</text> }">
            <a class="page-link" asp-action="AllFiltered" asp-controller="Rooms" asp-route-page="@Model.NextPageNumber">Next</a>
        </li>
    </ul>
</nav>
</div>


